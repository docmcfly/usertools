plugin.tx_usertools {
	view {
		templateRootPaths {
			0 = EXT:usertools/Resources/Private/Templates/
			1 = {$plugin.tx_usertools.view.templateRootPath}
		}

		partialRootPaths {
			0 = EXT:usertools/Resources/Private/Partials/
			1 = {$plugin.tx_usertools.view.partialRootPath}
		}

		layoutRootPaths {
			0 = EXT:usertools/Resources/Private/Layouts/
			1 = {$plugin.tx_usertools.view.layoutRootPath}
		}
	}
}

